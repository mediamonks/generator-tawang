{
  "name": "@mediamonks/generator-tawang",
  "version": "0.2.0",
  "description": "Generate a Tawang project used for developing with webpack inside Facebook's AR Studio.",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/mediamonks/tawang",
  "author": {
    "name": "Tim Struthoff",
    "email": "tim.struthoff@gmail.com",
    "url": "tst.works"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman",
    "generator",
    "mediamonks",
    "tawang",
    "webpack",
    "arstudio",
    "yeoman-generator"
  ],
  "devDependencies": {
    "debug": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^6.1.1",
    "nsp": "^3.2.1",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "crc-32": "^1.2.0",
    "is-domain-name": "^1.0.1",
    "needle": "^2.2.2",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/mediamonks/generator-tawang.git",
  "license": "MIT"
}
